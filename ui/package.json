{
  "name": "gymbyui",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn dev:vite & nodemon --ext css,scss,sass --watch src/ --exec yarn run dev:lint",
    "dev:vite": "vite",
    "dev:lint": "stylelint \"src/**/*.+(css|scss|sass)\" --config \"../.stylelintrc.json\" --fix --cache --cache-location \"../.stylelintcache\"",
    "build": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^1.3.4",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "nodemon": "^2.0.14",
    "stylelint": "^14.0.1",
    "tailwindcss": "^2.2.19",
    "vite": "^2.6.4"
  }
}
